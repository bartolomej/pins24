fun main()=
    let
        var mstThen="Condition is true\n\00"
        var msgElse="Condition is false\n\00"
    in
        if (getint())
        then
            putstr(^mstThen)
        else
            putstr(^msgElse)
        end,

        0
    end

fun exit(exitcode)
fun getint()
fun putint(value)
fun getstr(straddr)
fun putstr(straddr)
fun new(size)
fun del(straddr)
