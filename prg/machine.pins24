fun main()=
    let
        var newlinestr = "\n\00"
    in
        putint(inc(1, 2, 3, 4)),
        putstr(^newlinestr),

        0
    end

fun inc(a, b, c, d) = a + b * c + d

fun exit(exitcode)
fun getint()
fun putint(value)
fun getstr(straddr)
fun putstr(straddr)
fun new(size)
fun del(straddr)
