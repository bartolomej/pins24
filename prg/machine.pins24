fun main()=
    let
        var str = "\01"
        var newlinestr = "\n\00"
        var strptr = 0
    in
        strptr = ^str,
        putint(-(strptr^)),
        putint(!(strptr^)),
        putstr(^newlinestr),

        0
    end

fun exit(exitcode)
fun getint()
fun putint(value)
fun getstr(straddr)
fun putstr(straddr)
fun new(size)
